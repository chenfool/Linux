:map <F5> : tabprevious<CR>
:map <F6> : tabnext<CR>
:map <F4> : tabnew .<CR>
:map <F2> : vsplit .<CR>
:map <F3> : split .<CR>
:map <F7>   <C-w><C-w>
:map <C-h> : vertical res -1<CR>
:map <C-l> : vertical res +1<CR>
:map <C-j> : res +1<CR>
:map <C-k> : res -1<CR>

colorscheme evening
set tabstop=3
set background=dark
set incsearch
set hlsearch
